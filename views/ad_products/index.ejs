<h2 class="page-header">Tất cả sản phẩm</h2>
<form action='/admin/products' method = "GET">
    <div class="form-row">
        <div class="form-item">
            <label>Tìm sản phẩm</label>
            <input type="text" name="title" value="<%= searchOption.title %>"/>
        </div>
    </div>
    <div class="form-row">
        <div class="form-item">
            <label>Ngày tạo sau đó</label>
            <input type="date" name="publishAfter" value="<%= searchOption.publishAfter %>"/>
        </div>

        <div class="form-item">
            <label>Ngày tạo trước đó</label>
            <input type="date" name="publishBefore" value="<%= searchOption.publishBefore %>"/>
        </div>
    </div>
    <div class="form-row form-row-end">
        <button class="btn btn-primary" type="submit">Search</button>
      </div>
</form>
<div>
    <br>
   <%- include('./../partials/bookGrid.ejs',{products:products,large:true})%>
</div>